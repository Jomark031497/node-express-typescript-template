{
  "name": "node-express-typescript-template",
  "version": "1.0.0",
  "description": "A template for Node, Express, and Typescript",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node --env-file=.env dist/index.js",
    "dev": "tsx watch --env-file=.env src/index.ts",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "keywords": [
    "node",
    "express",
    "typescript"
  ],
  "author": "Jomark Pangan",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "date-fns": "^3.0.6",
    "express": "^4.18.2",
    "pino": "^8.17.1",
    "pino-pretty": "^10.3.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
